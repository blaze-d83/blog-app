
package templates

templ Page(title string) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<title>{ title }</title>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
			<link rel="stylesheet" href="/static/dist/output.css"/>
			<script src="https://unpkg.com/htmx.org@1.9.10" defer></script>
		</head>
		<body>
			<div>
				{ children }
			</div>
		</body>
	</html>
}
